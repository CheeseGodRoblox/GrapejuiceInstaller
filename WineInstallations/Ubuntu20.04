sudo apt remove wine-mono -y
sudo apt remove wine-gecko -y
sudo apt remove winetricks -y
sudo apt install libfaudio:i386 -y
sudo apt install libfaudio0 -y

sudo dpkg --add-architecture i386
wget -nc https://dl.winehq.org/wine-builds/winehq.key
